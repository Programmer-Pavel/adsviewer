    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fomantic-ui": "^2.8.7",
    "inquirer": "^7.3.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prisma": "^2.17.0",
    "url-loader": "^4.1.1"
  }
