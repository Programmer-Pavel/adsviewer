  "scripts": {
    "dev": "nodemon server.js",
    "next": "next",
    "build": "next build",
    "start": "next start",
    "now-build": "npm run semantic:build && next build",
    "semantic:build": "cd .semantic && gulp build-css build-assets",
    "semantic:watch": "cd .semantic && npm run semantic:build && gulp watch",
    "clean": "rimraf --no-glob ./.next ./out",
    "check:es:build": "es-check es5 './.next/static/**/*.js' -v",
    "escheck": "npm run clean && npm run build && npm run check:es:build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^2.17.0",
    "ajv": "^7.0.3",
    "ajv-i18n": "^3.6.0",
    "awilix": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "next": "^10.0.4",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^6.0.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "semantic-ui-react": "^2.0.1",
    "serverless-mysql": "^1.5.4",
    "styled-components": "^5.2.1",
    "uniforms": "^3.1.0",
    "uniforms-bridge-json-schema": "^3.1.0",
    "uniforms-semantic": "^3.1.0"
  },
